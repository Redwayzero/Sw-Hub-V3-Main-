	Name = "Auto Speed",
	Default = false,
	Callback = function(Value)
		_G.autoSpeed = Value
		autoSpeed()
	end    
})
 



--Buttons

ShopTab:AddToggle({
    Name = "Auto Buy Champion",
    Default = false,
    Callback = function(Value)
        isToggled = Value -- Update the toggle state when it's changed

        local args = {
            [1] = "Roll",
            [2] = "Champion Capsule",
            [3] = 1
        }

        local remoteEvent = game:GetService("ReplicatedStorage"):WaitForChild("Events"):WaitForChild("Champions/RemoteEvent")

        local delayBetweenCalls = 10

        while isToggled do -- Only execute the loop when the toggle is on
            for i = 1, 10 do 
                args[3] = i 
                remoteEvent:FireServer(unpack(args))
                wait(delayBetweenCalls)
            end
        end
    end
})

